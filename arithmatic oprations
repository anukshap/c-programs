#include <stdio.h>
int main() 
{
int a,b;
float c;
printf("enter a number of a and b\n");
scanf("%d%d",&a,&b);
c=a+b;
printf("%f\n",c);
if(a>b)
c=a-b;
else
c=b-a;
printf("%f\n",c);
c=b-a;
printf("%f\n",c);
c=a%b;
printf("%f\n",c);
c=b%a;
printf("%f\n",c);
c=a/b;
printf("%f\n",c);
c=b/a;
printf("%f\n",c);
c=a++;
printf("%f\n",c);
c=a--;
printf("%f\n",c);
c=b--;
printf("%f\n",c);
c=b++;
printf("%f\n",c);
c=a*b;
printf("%f\n",c);
}
