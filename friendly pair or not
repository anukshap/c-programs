#include<stdio.h>
int main() {
    int n1,n2,sum1=0,i,sum2=0;
printf("enter a  number n1 and n2 \n");
scanf("%d%d",&n1,&n2);
for(i=1;i<n1;i++)
{
if(n1%i==0)
sum1=sum1+i;

}
for(i=1;i<n2;i++)
{
if(n2%i==0)
sum2=sum2+i;

}

if(sum1==n1 && sum2==n2)
printf("a number is a friendly pair");


else
printf("a number is a  not friendly pair ");
}
